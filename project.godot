config_version=5

[application]

config/name="TopDown2DGame"
run/main_scene="res://Scenes/Runtime/UI/MainMenuGroup.tscn"
config/features=PackedStringArray("4.2", "C#", "Mobile")
config/icon="res://icon.svg"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/resizable=false
window/stretch/mode="viewport"
window/stretch/aspect="expand"
window/stretch/scale_mode="integer"

[dotnet]

project/assembly_name="TopDown2DGame"

[editor]

naming/default_signal_callback_name="On{NodeName}{SignalName}"
naming/default_signal_callback_to_self_name="On{SignalName}"
naming/scene_name_casing=1

[file_customization]

folder_colors={
"res://Content/": "yellow",
"res://Content/Sprites/": "purple",
"res://Scenes/": "red",
"res://Scenes/Runtime/": "green",
"res://Scripts/": "red",
"res://Scripts/Runtime/": "green"
}

